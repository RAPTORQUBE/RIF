/*
 * accessibility
 * Author: Bob van Luijt
 */
jQuery(document).ready(function($) {
    if( $('.accessibility-link').length ){
    	$('.accessibility-link').keypress(function(e) {
    		if(e.which == 13) {
	    		$(window).scrollTop( $('main').first('div').offset().top );
    		}
    	});
    }
    if( $('.right-off-canvas-toggle').length ){
    	$('.right-off-canvas-toggle').click(function(e) {
	    	setTimeout(function(){
	    		if( $('.right-off-canvas-toggle').attr('aria-expanded')=='true' ){
	    			$('.main-navigation').find('a').first().focus();
    			}
    		}, 500);
    	});
    }
});
</script>

